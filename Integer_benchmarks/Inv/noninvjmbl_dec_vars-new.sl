(set-logic LIA)
(declare-var x Int)
(declare-var n Int)
(declare-var v1 Int)
(declare-var v2 Int)
(declare-var v3 Int)
(declare-var x! Int)
(declare-var n! Int)
(declare-var v1! Int)
(declare-var v2! Int)
(declare-var v3! Int)
(synth-fun inv-f((parameter0 Int)(parameter1 Int)(parameter2 Int)(parameter3 Int)(parameter4 Int))Bool) 
(constraint (=> (= x n)(inv-f x n v1 v2 v3 )))
(constraint (=> (and (inv-f x n v1 v2 v3 ) (and (and (> x 1) (= x! (- x 1)) ) (= n! n) ) )(inv-f x! n! v1! v2! v3! )))
(constraint (=> (inv-f x n v1 v2 v3 )(not (and (<= x 1) (and (not (= x 1)) (>= n 0) ) ))))
(check-synth)

