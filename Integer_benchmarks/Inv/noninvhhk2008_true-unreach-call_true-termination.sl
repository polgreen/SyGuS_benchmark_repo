(set-logic LIA)
(declare-var a Int)
(declare-var b Int)
(declare-var res Int)
(declare-var cnt Int)
(declare-var a! Int)
(declare-var b! Int)
(declare-var res! Int)
(declare-var cnt! Int)
(synth-fun inv-f((parameter0 Int)(parameter1 Int)(parameter2 Int)(parameter3 Int))Bool) 
(constraint (=> (and (<= a 1000000) (<= 0 b) (<= b 1000000) (= res a) (= cnt b) )(inv-f a b res cnt )))
(constraint (=> (and (inv-f a b res cnt ) (and (> cnt 0) (= cnt! (- cnt 1)) (= res! (+ res 1 )) (= a! a) (= b! b) ) )(inv-f a! b! res! cnt! )))
(constraint (=> (inv-f a b res cnt )(or (> cnt 0) (= res (+ a b )) )))
(check-synth)

