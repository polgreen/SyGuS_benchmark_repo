(synth-fun inv-f((parameter0 Int)(parameter1 Int)(parameter2 Int)(parameter3 Int)(parameter4 Int)(parameter5 Int))Bool) 
(constraint (=> (and (= sn 0) (= i 1) )(inv-f i n sn v1 v2 v3 )))
(constraint (=> (and (inv-f i n sn v1 v2 v3 ) (and (= n! n) (and (= i! (+ i 1 )) (and (<= i n) (= sn! (+ sn 1 )) ) ) ) )(inv-f i! n! sn! v1! v2! v3! )))
