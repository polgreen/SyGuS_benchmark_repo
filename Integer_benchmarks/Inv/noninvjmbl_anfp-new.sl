(set-logic LIA)
(declare-var x Int)
(declare-var y Int)
(declare-var x! Int)
(declare-var y! Int)
(synth-fun inv-f((parameter0 Int)(parameter1 Int))Bool) 
(constraint (=> (and (= x 1) (= y 0) )(inv-f x y )))
(constraint (=> (and (inv-f x y ) (and (and (< y 100000) (= x! (+ x y )) ) (= y! (+ y 1 )) ) )(inv-f x! y! )))
(constraint (=> (inv-f x y )(not (and (>= y 100000) (< x y) ))))
(check-synth)

