(constraint (=> (and (= i 0) (and (= c 0) (> n 0) ) )(inv-f i c n )))
(constraint (=> (and (inv-f i c n ) (or (and (>= i n) (and (= i! i) (and (= c! c) (= n! n) ) ) ) (and (< i n) (and (= i! (+ i 1 )) (and (= c! (+ c i )) (= n! n) ) ) ) ) )(inv-f i! c! n! )))
