(set-logic LIA)
(declare-var x Int)
(declare-var y Int)
(declare-var x! Int)
(declare-var y! Int)
(synth-fun inv-f((parameter0 Int)(parameter1 Int))Bool) 
(constraint (=> (and (= x 0) (= y 50) )(inv-f x y )))
(constraint (=> (and (inv-f x y ) (and (< x 100) (ite (< x 50) (= x! (+ x 1 )) (= y! y)) (= x! (+ x 1 )) (= y! (+ y 1 )) ) )(inv-f x! y! )))
(constraint (=> (inv-f x y )(or (< x 100) (= y 100) )))
(check-synth)

